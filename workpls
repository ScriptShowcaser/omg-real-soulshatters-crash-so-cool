local tping = false
game:GetService("TeleportService").TeleportInitFailed:Connect(function(player, teleportResult, errorMessage)
    tping = false
    warn("Trying Again")
end)
function Teleport()
    if tping then return end
    tping = true
    spawn(function()
        warn("Getting all servers table.")
        local sl = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/".. game.PlaceId.. "/servers/Public?sortOrder=Asc&limit=100"))
        warn("Got all servers table.")
        local avservers = {}
        warn("Teleporting")
        for i,v in pairs(sl.data) do
           if v.playing ~= v.maxPlayers then
               table.Insert(avservers,1,v)
           end
        end
        game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, avservers[math.random(1,#avservers)].id, game.Players.LocalPlayer)
    end)
end
local lastping = 0
local serverhopcount = 0
spawn(function()
    while true do
        local ping = task.wait()
        spawn(function()
            local ae = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString(math.round(2/ping))
            for v in ae:gmatch("%d+") do
                lastping = tonumber(v)
                ae = tonumber(v)
                break
            end
            if serverhopcount > 400 then
                Teleport()
            elseif serverhopcount == lastping then
                serverhopcount = serverhopcount + 1 
            else
                serverhopcount = 0
            end
        end)
    end
end)

repeat task.wait() until game and game:IsLoaded()

warn("bye ss.. made by leaderr#8003")

